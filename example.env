OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_gemini_api_key_here
# AWS Bedrock: Use AWS CLI (aws configure) or set credentials below
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-1
MODEL=claude-sonnet-4-5-20250929 # requires Anthropic API key, or use anthropic.claude-sonnet-4-5-20250929-v1:0 for Bedrock
GRADER_MODEL=claude-sonnet-4-5-20250929 # Model to use for grading evaluations (optional, falls back to MODEL if not set)
ENV=DEV # or PROD
DEBUG=False # Set to True to enable debug mode in the terminal
LANGCHAIN_DEBUG=False # Set to True to enable debug mode in Langchain
